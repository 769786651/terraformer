{
  "name": "@terraformer/arcgis",
  "description": "Tools to convert ArcGIS JSON geometries to GeoJSON geometries and vica-versa.",
  "version": "2.0.0",
  "author": "Patrick Arlt <patrick.arlt@gmail.com>",
  "bugs": {
    "url": "https://github.com/terraformer-js/terraformer/issues"
  },
  "contributors": [
    "John Gravois <john@esri.com>"
  ],
  "files": [
    "index.js",
    "dist/arcgis.umd.js.map"
  ],
  "homepage": "https://github.com/terraformer-js/terraformer",
  "keywords": [
    "arcgis",
    "convert",
    "geo",
    "geojson",
    "geometry"
  ],
  "license": "Apache-2.0",
  "main": "dist/arcgis.umd.js",
  "unpkg": "dist/arcgis.umd.js",
  "module": "index.js",
  "jspm": {
    "main": "index.js",
    "registry": "npm",
    "format": "es6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/terraformer-js/terraformer"
  },
  "scripts": {
    "build": "rollup index.js -m dist/arcgis.umd.js.map -f umd -o dist/arcgis.umd.js -n TerraformerArcGIS",
